{
  "name": "generator-videojs-plugin",
  "version": "2.2.0",
  "description": "A Yeoman generator for VideoJS plugins",
  "license": "Apache-2.0",
  "main": "generators/app/index.js",
  "bin": {
    "vjsplugincheck": "bin/videojs-plugin-check.js",
    "vjsgenclean": "bin/videojs-generator-cleanup.js"
  },
  "repository": "videojs/generator-videojs-plugin",
  "author": {
    "name": "Pat O'Neill",
    "email": "pgoneill@gmail.com",
    "url": "https://github.com/misteroneill"
  },
  "engines": {
    "node": ">=4.4.0"
  },
  "vjsstandard": {
    "ignore": [
      "docs",
      "generators/app/templates"
    ]
  },
  "scripts": {
    "lint": "vjsstandard",
    "pretest": "npm run lint",
    "test": "mocha test/*.test.js",
    "version": "chg release -y && git add CHANGELOG.md"
  },
  "keywords": [
    "yeoman-generator",
    "videojs",
    "videojs-plugin"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "chg": "^0.3.2",
    "fs-extra": "^0.26.4",
    "glob": "^6.0.1",
    "lodash": "^4.17.1",
    "npm-run-all": "~1.2.0",
    "semver": "^5.1.0",
    "shelljs": "^0.5.3",
    "tap": "^2.3.1",
    "tap-parser": "^1.2.2",
    "tsmlj": "^1.0.0",
    "yeoman-generator": "^0.20.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "doctoc": "^0.15.0",
    "ghooks": "^1.3.2",
    "mocha": "^2.3.4",
    "videojs-standard": "^5.0.0"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint -- --errors"
    }
  }
}
